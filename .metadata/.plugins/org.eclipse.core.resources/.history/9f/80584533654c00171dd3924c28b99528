package com;

import java.util.Scanner;

public class GC1 {
	
	
	public static void main(String[] args) throws ClassNotFoundException {
		
		
		Scanner sc=new Scanner(System.in);
		String clName;
		System.out.println("Enter a Class");
		clName=sc.next();
		
		System.out.println(clName);
		
	
		Class.forName(clName);
		
		
		
		
		
	}
	

}
