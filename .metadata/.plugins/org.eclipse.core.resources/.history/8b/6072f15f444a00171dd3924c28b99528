package com;

/*
 * 
 * imp notes about strings
 * ----------------------------
 * 
 *  --> can create with/without 'new' operator
 * 
 */

public class String_Learn {
	public static void main(String[] args) {

		String s1 = "nexave";
		String s2 = "nexave";

		System.out.println(s1 == s2);

		String s3 = new String("nexwave");
		String s4 = new String("nexwave");

	}

}
