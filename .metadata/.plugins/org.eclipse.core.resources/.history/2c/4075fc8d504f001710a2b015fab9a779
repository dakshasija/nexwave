package com.multi.level2;

class Xyz {

	public synchronized static void sta() {

	}

}

public class Sync_Recap {
	public static void main(String[] args) {

		Thread thread1 = new Thread(() -> {
			Xyz.sta();
		}, "T1");
		Thread thread2 = new Thread(() -> {
			Xyz.sta();
		}, "T2");
		thread1.start();
		thread2.start();

	}

}
