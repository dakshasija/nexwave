package com;
//

//interface Gender {

//	String MALE = "male";
//	String FEMALE = "female";
//}

enum Gender {
	MALE, FEMALE
}

class Employee {
	String name;
	Gender gender; // MALE | FEMALE
}

public class Enum_Ex {
	public static void main(String[] args) {

		Employee employee = new Employee();
		employee.gender = Gender.FEMALE;

		// -------------------------------------------

		System.out.println(Gender.MALE.ordinal());

	}

}
