package com.multi;

class PrintNum implements Runnable {
	@Override
	public void run() {
		String name = Thread.currentThread().getName();
		for (int i = 0; i < 500; i++) {
			System.out.println(name + "==>" + i);
		}
	}
}

public class Thread_Scheduler_Demo {

	public static void main(String[] args) {

	}

}
