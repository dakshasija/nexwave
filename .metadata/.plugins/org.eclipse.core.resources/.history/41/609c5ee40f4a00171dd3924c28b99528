package com;

class A extends Object{
	public A() {
		super();
		System.out.println("im A");
	}
}

class B extends A {
	public B() {
		super();
		System.out.println("Im B");
	}
}

class C extends B {
	public C() {
		super();
		System.out.println("im C");
	}
}

public class Constructor_Exe_Flow {
	public static void main(String[] args) {
		C c = new C();
	}
}
