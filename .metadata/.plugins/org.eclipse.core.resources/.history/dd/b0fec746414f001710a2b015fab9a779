package com.multi.level2;

class B {

	void m1() {
		System.out.println("m1()...");
	}

	void m2() {
		System.out.println("m2()...");
	}

	static void m3() {
		System.out.println("m3()...");
	}

}

public class Sync_Block_Ex {

	public static void main(String[] args) {

		B b1 = new B();
		B b2 = new B();

		Thread thread1 = new Thread(() -> {
			b1.m1();
		}, "T1");
		Thread thread2 = new Thread(() -> {
			b2.m1();
		}, "T2");

	}
}
