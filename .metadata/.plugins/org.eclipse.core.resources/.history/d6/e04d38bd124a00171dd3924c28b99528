package com;

import com.god.God;
import com.live.Animal;
import com.live.Human;

public class App {

	public static void main(String[] args) {

		God god = new God();

		Human human = new Human();
		Animal animal = new Animal();

		god.manageHuman(human);
		System.out.println();
		god.manageAnimal(animal);

	}

}
